{
  "name": "antd-vesc",
  "displayName": "Ant Design Pro For VSCode",
  "version": "1.0.10",
  "description": "效能开发",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ant-design/antd-vsce.git"
  },
  "publisher": "qixiancs",
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "lint": "eslint src --ext ts",
    "package": "webpack --mode production --devtool hidden-source-map && npm run tsc",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "pnpm run package",
    "prettier": "prettier -c --write \"**/*.{css,less,js,ts,tsx,conf}\"",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out",
    "tsc": "tsc -p ./server"
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:json",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "dependencies": {
    "@ant-design/doc": "^1.0.6",
    "@babel/core": "^7.23.9",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@types/babel__traverse": "^7.20.5",
    "execa": "^7.1.1",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/git-url-parse": "^9.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-two-style-order": "^1.0.1",
    "@types/vscode": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "author": {
    "chenshuai2144": "qixian.cs@outlook.com"
  },
  "icon": "images/icon.png"
}
